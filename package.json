{
  "name": "nofilter",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^1.9.1",
    "colors": "^1.1.2",
    "eslint": ">=3.0.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": ">=3.3.0",
    "eslint-plugin-react": ">=2.7.1",
    "eslint-plugin-standard": ">=2.0.0",
    "express": "^4.15.2",
    "flow-bin": "^0.40.0",
    "flow-typed": "^2.0.0",
    "flow-webpack-plugin": "^0.3.6",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react": "16.0.0-alpha.6",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-native": "^0.43.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "webpack": "2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack --optimize-minimize",
    "start": "npm run clean && npm-run-all --parallel start:server start:app watch:server watch:app",
    "start:server": "nodemon --config nodemon-server.json",
    "start:app": "react-native start >/dev/null",
    "watch:server": "webpack --watch",
    "watch:app": "nodemon --config nodemon-app.json",
    "clean": "rm -rf build 2>/dev/null",
    "install:android": "react-native run-android --no-packager --root platform"
  }
}
